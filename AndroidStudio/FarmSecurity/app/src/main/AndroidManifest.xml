<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.farmsecurity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FarmSecurity">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main" android:label="메인화면"/>
        <activity android:name=".Find_idpw" android:label="아이디/비밀번호 찾기"/>
        <activity android:name=".Signup" android:label="회원가입"/>
        <activity android:name=".Loginform" android:label="로그알림"/>
        <activity android:name=".Logcheck" android:label="로그확인"/>
        <activity android:name=".change_pw" android:label="비밀번호 변경"/>
    </application>

</manifest>